POSTGRES_PORT=5432
POSTGRES_DB=postgres
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres

APP_PORT=5000
WB_API_KEY=your_api_key_here

# Either base64-encoded JSON credentials or a path inside the container
GOOGLE_SA_CREDENTIALS=
GOOGLE_SA_PATH=

# Wildberries retry/backoff configuration
# Maximum number of retries for API requests (default 3)
WB_MAX_RETRIES=3
# Backoff base in milliseconds (default 1000)
WB_BACKOFF_BASE_MS=1000
# Backoff max in milliseconds (default 30000)
WB_BACKOFF_MAX_MS=30000